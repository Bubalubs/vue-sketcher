{
    "name": "vue-sketcher",
    "version": "1.1.1",
    "description": "A Vue 3 component for drawing and sketching on an HTML canvas",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "build-demo": "vite build --mode demo",
        "preview-demo": "vite preview --outDir demo/dist",
        "lint": "eslint \"**/*.{ts,vue}\"",
        "lint:fix": "eslint \"**/*.{ts,vue}\" --fix",
        "format": "prettier --write ."
    },
    "keywords": [
        "vue",
        "canvas",
        "drawing",
        "sketching"
    ],
    "author": "Adam Glaysher <adam@bubalubs.io>",
    "license": "MIT",
    "dependencies": {
        "vue": "^3.5.13"
    },
    "devDependencies": {
        "@eslint/config-array": "^0.19.0",
        "@eslint/object-schema": "^2.1.0",
        "@typescript-eslint/eslint-plugin": "^8.21.0",
        "@vitejs/plugin-vue": "^5.2.1",
        "@vue/compiler-sfc": "^3.5.13",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.0",
        "eslint-plugin-prettier": "^5.2.0",
        "eslint-plugin-vue": "^9.32.0",
        "glob": "^11.0.0",
        "prettier": "^3.4.0",
        "rimraf": "^6.0.0",
        "typescript": "^5.7.0",
        "vite": "^6.0",
        "vue-tsc": "^2.2"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ]
}
